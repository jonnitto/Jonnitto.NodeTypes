prototype(NT:DateOutput) < prototype(Fusion:Template){
	templatePath = ${'resource://Jonnitto.NodeTypes/Private/Templates/FusionObjects/DateOutput.html'}
	locale = ${node.context.targetDimensions.language}
	date = ${date}
}

prototype(NT:DayOutput) < prototype(NT:DateOutput){
	sectionName = 'day'
}

prototype(NT:MonthOutput) < prototype(NT:DateOutput){
	sectionName = 'month'
}
