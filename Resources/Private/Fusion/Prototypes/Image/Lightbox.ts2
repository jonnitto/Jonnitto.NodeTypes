prototype(NT:LightboxAttributes) < prototype(Fusion:Attributes) {
	@context.asset = ${asset ? asset : property ? q(node).property(property) : q(node).property('image')}
	target = ${configJonnittoNodeTypes.Image.lightbox.target}
	class = ${configJonnittoNodeTypes.Image.lightbox.class}
	data-size = Fusion:Array {
		width = ${asset.width}
		divider = 'x'
		height = ${asset.height}
	}
	href = ImageUri {
		asset = ${asset}
	}
}
